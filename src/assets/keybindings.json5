[
  { key: "Tab", command: "focusNextPanel", when: "filePanel.focus || quickView.focus" },
  { key: "Shift+Tab", command: "focusPrevPanel", when: "filePanel.focus || quickView.focus" },
  { key: "Escape", command: "focusActivePanel", when: "terminal.focus" },
  { key: "ArrowUp", command: "cursorUp", when: "filePanel.focus" },
  { key: "ArrowDown", command: "cursorDown", when: "filePanel.focus" },
  { key: "ArrowLeft", command: "cursorLeft", when: "filePanel.focus" },
  { key: "ArrowRight", command: "cursorRight", when: "filePanel.focus" },
  { key: "Home", command: "cursorStart", when: "filePanel.focus" },
  { key: "End", command: "cursorEnd", when: "filePanel.focus" },
  { key: "PageUp", command: "cursorPageUp", when: "filePanel.focus" },
  { key: "PageDown", command: "cursorPageDown", when: "filePanel.focus" },
  { key: "Shift+ArrowUp", command: "cursorUp", when: "filePanel.focus", args: { "select": true } },
  { key: "Shift+ArrowDown", command: "cursorDown", when: "filePanel.focus", args: { "select": true } },
  { key: "Shift+ArrowLeft", command: "cursorLeft", when: "filePanel.focus", args: { "select": true } },
  { key: "Shift+ArrowRight", command: "cursorRight", when: "filePanel.focus", args: { "select": true } },
  { key: "Shift+Home", command: "cursorStart", when: "filePanel.focus", args: { "select": true } },
  { key: "Shift+End", command: "cursorEnd", when: "filePanel.focus", args: { "select": true } },
  { key: "Shift+PageUp", command: "cursorPageUp", when: "filePanel.focus", args: { "select": true } },
  { key: "Shift+PageDown", command: "cursorPageDown", when: "filePanel.focus", args: { "select": true } },
  { key: "Enter", command: "open", when: "filePanel.focus" },
  { key: "Space", command: "focusTerminal", when: "filePanel.focus" },
  { key: "Shift+Enter", command: "openShell", when: "filePanel.focus" },
  { key: "Ctrl+PageUp", command: "dirUp", when: "filePanel.focus" },
  { key: "Alt+PageUp", command: "dirUp", when: "filePanel.focus" },
  { key: "F5", command: "copyFiles", when: "filePanel.focus" },

  { key: "ArrowUp", command: "cursorEnd", when: "filePanel.focus && filePanel.firstItem" },
  { key: "ArrowDown", command: "cursorStart", when: "filePanel.focus && filePanel.lastItem" },

  { key: "Ctrl+KeyO", command: "togglePanels" },
  { key: "Ctrl+KeyD", command: "switchDevMode" },
]
